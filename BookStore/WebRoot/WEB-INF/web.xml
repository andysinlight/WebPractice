<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
	
	<servlet>
		<servlet-name>clientServiet</servlet-name>
		<servlet-class>com.yuan.controller.ClientServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>manageServlet</servlet-name>
		<servlet-class>com.yuan.controller.ManageServlet</servlet-class>
	</servlet>
	
	
	
	<filter>
		<filter-name>privilegeFilter</filter-name>
		<filter-class>com.yuan.filter.PrivilegeFilter</filter-class>
	</filter>
	
	
	
	
	
	<servlet-mapping>
		<servlet-name>clientServiet</servlet-name>
		<url-pattern>/servlet/clientServiet</url-pattern>
	</servlet-mapping>	
	
	<servlet-mapping>
		<servlet-name>manageServlet</servlet-name>
		<url-pattern>/servlet/manageServlet</url-pattern>
	</servlet-mapping>	
	
	
	<filter-mapping>
		<filter-name>privilegeFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
	
	
	
	
	
	
	

</web-app>
